<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>id3</name>
  <summary>Functions to read and write ID3 tags in MP3 files.</summary>
  <description>id3 enables to to retrieve and update information from ID3 tags in
MP3 files. It supports version 1.0 and version 1.1.</description>
  <maintainers>
    <maintainer>
      <user>schst</user>
      <name>Stephan Schmidt</name>
      <email>schst@php-tools.net</email>
      <role>lead</role>
    </maintainer>
    <maintainer>
      <user>luckec</user>
      <name>Carsten Lucke</name>
      <email>luckec@php.net</email>
      <role>developer</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.1</version>
    <date>2004-06-18</date>
    <license>PHP License</license>
    <state>alpha</state>
    <notes>initial pecl release</notes>
    <deps>
      <dep type="php" rel="ge" version="4.3.0" optional="no"/>
    </deps>
    <configureoptions>
      <configureoption name="enable-id3" default="autodetect" prompt="Enable id3 support?"/>
    </configureoptions>
    <filelist>
      <dir baseinstalldir="" name="/">
       <dir name="examples">
        <file role="doc" name="example_get.php"/>
        <file role="doc" name="example_get_genre_name.php"/>
        <file role="doc" name="example_set.php"/>
        <file role="doc" name="example_version.php"/>
       </dir>
       <file role="doc" name="CREDITS"/>
       <file role="doc" name="EXPERIMENTAL"/>
       <file role="doc" name="TODO"/>
       <file role="src" name="config.m4"/>
       <file role="src" name="config.w32"/>
       <file role="src" name="id3.c"/>
       <file role="src" name="php_id3.h"/>
      </dir>
    </filelist>
  </release>
  <changelog>
    <release>
      <version>0.1</version>
      <date>2004-06-18</date>
      <license>PHP License</license>
      <state>alpha</state>
      <notes>initial pecl release</notes>
    </release>
  </changelog>
</package>
